FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
chemistryType
{
    chemistrySolver   ode;
    chemistryThermo   psi;
}

chemistry       on;

initialChemicalTimeStep 1e-8;

EulerImplicitCoeffs
{
    cTauChem        1;
    equilibriumRateLimiter off;
}

odeCoeffs
{
    solver          RKCK45;
    absTol          1e-12;
    relTol          1e-1;
}


// ************************************************************************* //
differentialDiffusion on;
thermalDiffusion off;
