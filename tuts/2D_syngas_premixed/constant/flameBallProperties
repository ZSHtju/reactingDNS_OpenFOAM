CCx     0.004;
CCy     0.004;
CCz     0.00004;
// for circle

amplitudeFactor  8; // real amplitude = (outerBunrtRadius-innerBunrtRadius)/amplitudeFactor;
frequency  8;  //  2*pi*omega


